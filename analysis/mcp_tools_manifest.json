        "required": [
          "table",
          "data"
        ]
      },
      "source": "supabase_tools.json",
      "operationId": "supabase_insert"
    },
    {
      "name": "supabase_update",
      "description": "Update rows in a table matching filter conditions.",
      "method": "PATCH",
      "path": "/{table}",
      "inputSchema": {
        "type": "object",
        "properties": {
          "table": {
            "type": "string"
          },
          "data": {
            "type": "object",
            "description": "Update data"
          },
          "filters": {
            "type": "object",
            "description": "WHERE conditions"
          }
        },
        "required": [
          "table",
          "data",
          "filters"
        ]
      },
      "source": "supabase_tools.json",
      "operationId": "supabase_update"
    },
    {
      "name": "supabase_delete",
