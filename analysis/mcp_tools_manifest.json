    {
      "name": "deploy_pages",
      "description": "Deploy a local build directory to a Cloudflare Pages project via Wrangler CLI.",
      "method": "POST",
      "path": "wrangler pages deploy",
      "inputSchema": {
        "type": "object",
        "properties": {
          "dist_path": {
            "type": "string",
            "description": "Local build output directory"
          },
          "project": {
            "type": "string",
            "description": "Pages project name"
          },
          "branch": {
            "type": "string",
            "default": "main",
            "description": "Branch label"
          },
          "commit_message": {
            "type": "string",
            "description": "Optional commit message for Wrangler"
          }
        },
        "required": [
          "dist_path",
          "project"
