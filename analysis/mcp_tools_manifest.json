          },
          "offset": {
            "type": "integer",
            "description": "Offset for pagination"
          },
          "order_by": {
            "type": "string",
            "description": "Order by column"
          },
          "order_direction": {
            "type": "string",
            "enum": [
              "asc",
              "desc"
            ]
          }
        },
        "required": [
          "table"
        ]
      },
      "source": "supabase_tools.json",
      "operationId": "supabase_select"
    },
    {
      "name": "supabase_insert",
      "description": "Insert a new row into a table.",
      "method": "POST",
      "path": "/{table}",
      "inputSchema": {
        "type": "object",
        "properties": {
          "table": {
            "type": "string"
          },
          "data": {
            "type": "object",
            "description": "Row data"
          },
          "return": {
            "type": "string",
            "enum": [
              "minimal",
              "representation"
