        "type": "object",
        "properties": {
          "table": {
            "type": "string"
          },
          "filters": {
            "type": "object",
            "description": "WHERE conditions"
          }
        },
        "required": [
          "table",
          "filters"
        ]
      },
      "source": "supabase_tools.json",
      "operationId": "supabase_delete"
    },
